{
  "manifest_version": 2,

  "name": "Hold My Page",
  "description": "This extension will bookmark the current page and save it to holdmypage.appspot.com",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_title": "Hold this page!",
   "default_popup": "holdmypage.html"
  },

  "background": {
    "scripts": ["jquery-2.1.4.min.js","holdmypage.js"],
    "persistent": true
  },

  "permissions": [
   "activeTab",
   "tabs",
   "http://localhost:8080/",
   "http://holdmypage.appspot.com/"
   ]
}